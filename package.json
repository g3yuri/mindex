{
  "name": "mindex",
  "version": "0.0.2",
  "description": "Mindex Web",
  "productName": "Mindex Web",
  "author": "Yuri Quispe <g3yuri@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "npm run build-pwa",
    "test": "npx playwright test",
    "test-rep": "npx playwright show-report",
    "build-only": "quasar build",
    "build-spa": "quasar build && rsync -zr --delete ./dist/spa/ ene:/www/wwwroot/gmi.gd.pe/spa/",
    "build-pwa": "quasar build -m pwa && rsync -zr --delete --exclude=.well-known ./dist/pwa/ ene:/www/wwwroot/gmi.gd.pe/pwa/",
    "icon-pwa": "icongenie g -m pwa -i ./src/assets/logo.png",
    "icon": "icongenie g -i ./src/assets/logo.png",
    "lint": "eslint --ext .js,.ts,.vue ./"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.1",
    "quasar": "^2.6.0",
    "v-viewer": "^3.0.11",
    "vue": "^3.0.0",
    "vue-echarts": "^6.5.4",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.5.4",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
